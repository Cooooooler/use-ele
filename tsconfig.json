{
  "compilerOptions": {
    "strict": true,
    "target": "ES2021",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "strictNullChecks": false,
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "use-ele": ["src", "docs/*"],
      "use-ele/*": ["src/*", "*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*"]
}
